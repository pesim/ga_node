doctype 5
html
  head
    title Google Analytics
    link(rel='stylesheet', href='http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css')
    link(rel='stylesheet', href='/stylesheets/bootstrap.min.css')
    script(src='//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js')
    script(src='//ajax.googleapis.com/ajax/libs/jqueryui/1.8.24/jquery-ui.min.js')
    script(src='http://code.highcharts.com/highcharts.js')
    script(src="/javascripts/bootstrap.min.js")
    script(src='/javascripts/chart.js')
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
  body
    .row
      .span8
        input#start-date(type='text', placeholder='Start Date')
        input#end-date(type='text', placeholder= 'End date', disabled)
      .span2
        a#plot.btn.btn-block.disabled(type='button') End Code
      .span2
        a#plot-sales.btn.btn-block.disabled(type='button') ItemSales
    #chart_area
